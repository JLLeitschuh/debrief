		<DebriefReplayObserver Name="Freq_Cuts" record_detections="true"
			record_positions="false" Active="true" record_decisions="false"
			file_name="Freq_Cuts" SubjectSensor="NB_FREQ">
		</DebriefReplayObserver>
	</ObserverList>
</ScenarioController>
