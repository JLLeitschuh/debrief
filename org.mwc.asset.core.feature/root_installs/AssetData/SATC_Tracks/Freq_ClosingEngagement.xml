	<ObserverList>
		<TimeObserver Name="Domesday Timer" Active="true">
			<Duration Value="90" Units="minutes" ></Duration>
		</TimeObserver>
		<DebriefReplayObserver Name="Freq_Tracks" record_detections="false"
