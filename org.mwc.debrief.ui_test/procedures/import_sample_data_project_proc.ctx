proc "import_sample_data_project_proc" {
get-menu -path "File/Import..." | click
with [get-window Import] {
    get-tree | select "General/Existing Projects into Workspace"
    get-button "Next >" | click
}
switch [get-os]
        [case "macosx" {            
           set-dialog-result Folder  [concat [str [get-java-property "osgi.install.area"] | invoke substring 6] "sample_data"]
        }]    
       
        -default {
           set-dialog-result Folder  [concat [str [get-java-property "osgi.install.area"] | invoke substring 5] "sample_data"]
        }
//set-dialog-result Folder  [concat [str [get-java-property "osgi.install.area"] | invoke substring [get-by-os -default "5" -macosx "6"]] "sample_data"]

with [get-window Import] {
    get-button "Browse..." | click
    get-button "Copy projects into workspace" | check
    get-button Finish | click
}
}
